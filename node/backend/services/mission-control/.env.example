# Redis Configuration
REDIS_URL=redis://127.0.0.1:6379

# Solana RPC Endpoints (comma-separated for multiple)
RPC_ENDPOINTS=https://api.mainnet-beta.solana.com,https://solana-api.projectserum.com

# TPU Endpoint
TPU_ENDPOINT=127.0.0.1:8003

# Validator Stake (in lamports)
VALIDATOR_STAKE=1000000000000

# ClickHouse Configuration
CLICKHOUSE_URL=http://localhost:8123
CLICKHOUSE_DATABASE=mission_control
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_TOPIC_PREFIX=mission_control

# Logging
RUST_LOG=info,mission_control=debug

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# Jito Configuration (optional)
JITO_AUTH_TOKEN=
JITO_TIP_ACCOUNT=

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090