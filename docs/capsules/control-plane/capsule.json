{
  "name": "control-plane",
  "version": "1.0.0",
  "description": "MEV Control Plane - Centralized command and control for SOTA MEV infrastructure",
  "type": "python-service",
  "port": 8000,
  "health_endpoint": "/health",
  "metrics_endpoint": "/metrics",
  "dependencies": [
    "clickhouse",
    "redis",
    "kafka"
  ],
  "capabilities": [
    "command-execution",
    "policy-management", 
    "model-swapping",
    "kill-switches",
    "cryptographic-signing",
    "real-time-control"
  ],
  "performance_targets": {
    "command_latency_p99_ms": 50,
    "throughput_commands_per_sec": 10000,
    "availability_pct": 99.99
  },
  "security": {
    "authentication": "ed25519",
    "authorization": "rbac", 
    "encryption": "tls-1.3",
    "audit_logging": true
  },
  "monitoring": {
    "prometheus_metrics": true,
    "distributed_tracing": true,
    "health_checks": true,
    "alerting": true
  },
  "deployment": {
    "container_image": "mev/control-plane:latest",
    "cpu_limit": "2000m",
    "memory_limit": "4Gi",
    "replicas": 3,
    "auto_scaling": true
  },
  "documentation": [
    "index.md",
    "env.md", 
    "commands.md",
    "api.md"
  ],
  "tags": [
    "control",
    "command",
    "critical",
    "high-availability"
  ]
}