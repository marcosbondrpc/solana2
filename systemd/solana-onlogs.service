[Unit]
Description=Solana logs ingestion â†’ ClickHouse
After=network.target backend-api.service
Wants=network-online.target

[Service]
Type=simple
EnvironmentFile=/opt/solana-realtime/uvicorn.env
WorkingDirectory=/opt/solana-realtime/app
ExecStart=/usr/bin/env bash -lc '\
  python3 -m services.ingestion.solana_onlogs.run \
'
Restart=always
RestartSec=3
NoNewPrivileges=true
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target