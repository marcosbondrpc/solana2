{
  "name": "@solana-mev/workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Legendary Solana MEV Infrastructure - Ultra-High Performance DeFi Dashboard",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check", 
    "clean": "turbo run clean",
    "proto:generate": "turbo run proto:generate",
    "lab-smoke-test": "make lab-smoke-test",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && turbo run build",
    "backend:dev": "make backend-dev",
    "backend:build": "cd backend && cargo build --release",
    "backend:test": "cd backend && cargo test --release",
    "dashboard:dev": "cd frontend/apps/dashboard && npm run dev",
    "operator:dev": "cd frontend/apps/operator && npm run dev",
    "services:start": "make sota-up",
    "services:stop": "make sota-down",
    "services:health": "make sota-health"
  },
  "workspaces": [
    "frontend/apps/*",
    "frontend/packages/*"
  ],
  "devDependencies": {
    "turbo": "^1.13.2",
    "@turbo/gen": "^1.13.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcosbondrpc/solana2.git"
  },
  "keywords": [
    "solana",
    "mev",
    "defi",
    "dashboard",
    "typescript",
    "react",
    "vite",
    "turborepo",
    "high-performance"
  ],
  "author": "Solana MEV Team",
  "license": "MIT"
}